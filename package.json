{
  "name": "@starkdefi/token-lists",
  "author": {
    "name": "StarkDefi Labs",
    "url": "https://starkdefi.com"
  },
  "description": "Spec Token Lists",
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Starkdefi/token-lists"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "postbuild": "cp src/tokenlist.schema.json dist/tokenlist.schema.json",
    "test": "tsdx test",
    "lint": "tsdx lint src test",
    "prepublishOnly": "yarn test && yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/token-lists.esm.js",
  "devDependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "husky": "^9.1.7",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
